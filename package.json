{
  "name": "kinvey-flex-ts-boilerplate",
  "version": "0.1.1",
  "description": "Typescript boilerplate for Kinvey Flex Services.",
  "main": "kinvey.ts",
  "scripts": {
    "kinvey": "cross-env-shell ts-node ./kinvey.ts",
    "kinvey-profile-list": "npm run kinvey profile list",
    "kinvey-flex-status": "npm run kinvey flex status",
    "kinvey-flex-deploy": "ts-node ./build-dist-pkg.ts && webpack && npm run kinvey flex deploy",
    "dev": "webpack && ts-node flex-dev-server.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dustintownsend/kinvey-flex-ts-boilerplate.git"
  },
  "keywords": [
    "kinvey",
    "flex-sdk",
    "typescript",
    "boilerplate"
  ],
