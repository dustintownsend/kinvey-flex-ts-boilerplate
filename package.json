  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dustintownsend/kinvey-flex-ts-boilerplate/issues"
  },
  "homepage": "https://github.com/dustintownsend/kinvey-flex-ts-boilerplate#readme",
  "dependencies": {
    "kinvey-flex-sdk": "^2.0.6",
    "request": "^2.83.0"
  },
  "devDependencies": {
    "@types/node": "^9.3.0",
    "cross-env": "^5.1.3",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "kinvey-cli": "https://github.com/Kinvey/kinvey-cli#v3.0.0-beta.0",
    "ts-loader": "^3.2.0",
    "ts-node": "^4.1.0",
